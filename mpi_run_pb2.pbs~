#!/bin/bash
#PBS -W group_list=uafaqua
#PBS -q standard_4
#PBS -l walltime=12:00:00
#PBS -l nodes=50:ppn=4
#PBS -j oe
/etc/profile.d/modules.sh
module load python/2.7.4
source $HOME/python/bin/activate
cd $CENTER/temp_gx_files
for gx in *_cleanHL.gx;do
	mpirun --mca mpi_warn_on_fork 0 -np 200  python $HOME/genx_mpi_qiu/GenX_run_multiple_mpi_new_3.py $gx 2>&1 |tee job.$PBS_JOBID.out
done
